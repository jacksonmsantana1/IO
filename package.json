{
  "name": "io",
  "version": "0.0.0-semantically-released",
  "description": "IO Monad for javascript",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "jscs": "jscs ./src/index.js",
    "eslint": "eslint ./src/index.js",
    "lint": "npm run jscs && npm run eslint",
    "pretest": "npm run test:build",
    "test:cover": "istanbul cover -x *.spec.js _mocha -- -R spec test/index.spec.js ",
    "test:unit": "mocha --compilers js:babel-core/register ./src/*.js --reporter nyan",
    "test:build": "babel --out-dir test src",
    "test": "npm run lint && npm run test:cover && npm run test:unit",
    "posttest": "rm -rf test",
    "check-coverage": "istanbul check-coverage --statements 70 --branches 70 --functions 70 --lines 70",
    "prebuild": "rm -rf dist index.*.js",
    "build": "npm-run-all --parallel 'build:*'",
    "build:main": "babel ./src -o ./index.js --ignore *.spec.js -s",
    "build:umd": "webpack --output-filename index.umd.js",
    "build:umd.min": "webpack --output-filename index.umd.min.js -p",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jacksonmsantana1/IO.git"
  },
  "author": "Jackson Martorano Sant'Ana",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jacksonmsantana1/IO/issues"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.11.5",
    "chai": "^3.5.0",
    "commitizen": "^2.8.2",
    "cz-conventional-changelog": "^1.1.6",
    "eslint": "^3.1.1",
    "eslint-config-airbnb-base": "^5.0.0",
    "eslint-plugin-import": "^1.11.1",
    "ghooks": "^1.3.2",
    "istanbul": "^0.4.4",
    "jscs": "^3.0.7",
    "mocha": "^2.5.3",
    "npm-run-all": "^2.3.0",
    "nyc": "^7.0.0",
    "ramda": "^0.21.0",
    "rimraf": "^2.5.4",
    "semantic-release": "^4.3.5",
    "webpack": "^1.13.1"
  },
  "config": {
    "commitzen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test"
    }
  },
  "homepage": "https://github.com/jacksonmsantana1/IO#readme",
  "dependencies": {
    "ramda": "^0.21.0"
  }
}
